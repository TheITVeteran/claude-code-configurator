{
  "mcpServers": {
    "test-mcp": {
      "type": "stdio",
      "command": "test-command",
      "args": ["--test"],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {}
    },
    "deepgraph-react": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-code-graph@latest", "facebook/react"],
      "env": {}
    },
    "deepgraph-typescript": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-code-graph@latest", "microsoft/TypeScript"],
      "env": {}
    },
    "github-integration": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {}
    },
    "filesystem-access": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/path/to/allowed/files"
      ],
      "env": {}
    },
    "postgresql-integration": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres"],
      "env": {}
    },
    "mysql-integration": {
      "type": "stdio",
      "command": "uvx",
      "args": ["mcp-server-mysql"],
      "env": {}
    },
    "web-fetch": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-fetch"],
      "env": {}
    }
  }
}
